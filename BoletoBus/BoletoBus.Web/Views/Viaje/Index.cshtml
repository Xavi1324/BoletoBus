@model IEnumerable<BoletoBus.Web.Models.Viaje.ViajeGetModelBase>

@{
    ViewData["Title"] = "Index";
}

<h1>Viaje List</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
