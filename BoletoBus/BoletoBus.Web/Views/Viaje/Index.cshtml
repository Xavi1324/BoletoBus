            <td>
                @Html.DisplayFor(modelItem => item.completo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fechaCreacion)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {  id=item.idViaje  }) |
                @Html.ActionLink("Details", "Details", new { id = item.idViaje }) |
                
            </td>
        </tr>
}
    </tbody>
</table>
